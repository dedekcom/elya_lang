		this.play(-1);
		if( cllen.ilen3( this.getname(), "anpiast" )<=60 )	{
			this.mdamage();
		}
	}
	mfinstwor2()	{
		this.mfinstwor();
		this.mrownaj();
	}
	mfinstwor()	{
		if( this.actionnr(-1)==il )	{
			if ( this.mokgo( this.getpx() + 5, this.getey() - 10 ) == true )	{
				this.move( -5, 0 );
			} else {
				this.play( ip );
			}
		} else {
			if ( this.mokgo( this.getex() - 5, this.getey() - 10 ) == true )	{
				this.move( 5, 0 );
			} else {
				this.play( il );
			}
		}
	}
