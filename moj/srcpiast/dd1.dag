		idgame = 0;
		dbn.load(sfile);
	}
	next(int n)	{
		idgame = dbn.findbyrow( gameapi.getgamename() ) + n;
		int ile = dbn.getrowsno();
		if( idgame < 0 )	{
			idgame = ile-1;
		} else if (idgame>=ile)	{
			idgame = 0;
		}
		gameapi.play( dbn.get( idgame, 0 ) );
	}
