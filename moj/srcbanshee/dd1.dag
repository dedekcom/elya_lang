			@s = grelsc.get(i);
			if( <s>.framenr!=i ) b=0;
		}
		if( b )	{
			.mskipmg;
		} else .advsetstdstate;
	}
	mskipmg	{
		clsave.bset("e1_mg00dzban_ulozony");
		anaskopis.anfadeout(500,null);
		.mshowmoon;
		sgmstate="state_open";
		animopen2.play(1);
