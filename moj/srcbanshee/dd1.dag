	additem(string sname, string sfile)	{
		if( arPocket.contains(sname) ) return;
		arPocket.add(sname);
		arPocketFile.add(sfile);
// 		string s = _sbase+iNumItems;
		string s = _sbase + sname;
		new anima <s>;
		this.add(s);
		<s>.load( sitempath + sfile + ".pyz" );
		//<s>.setframe("in",0);
		<s>.play(0);
