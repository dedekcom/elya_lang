		if(grbuts.isinfunc(x,y,"isbutin") )	{
			@s = grbuts.getsfound;
			if( s!=sobject )	{
				.lastoff;
				sobject = s;
				.<this + "_moveon">;
 				if( <s>.hasaddedmet("butmoveon") )
					<s>.butmoveon;
			}
		} else {
			.lastoff;
		}
	}
	lastoff	{
		if( sobject!=null )	{
			.<this + "_moveoff">;
			 
			if( <sobject>.hasaddedmet("butmoveoff") )
				<sobject>.butmoveoff;
				
			/*if( .checktouchpad &&  <sobject>.hasaddedmet("butrel") )
			{
				 <sobject>.butrel;
				.<this + "_lrel">;
			}*/
			sobject = null;
		}
	}
