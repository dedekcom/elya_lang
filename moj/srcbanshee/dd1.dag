		bsystem_cursor = true;
		int x = mouse.getpx;
		int y = mouse.getpy;
		mouse.hide;
		mouse.showsys;
		//mouse.setpos(x, y);
		ancursor_check.show;
		engine.refresh;
		engine.redraw;
	}
	mshowsoftmouse	{
		bsystem_cursor = false;
		|int x, int y| = mouse.getsyspos;
		mouse.hidesys;	
		mouse.show;
		mouse.setpos(x, y);
		ancursor_check.hide;
		engine.refresh;
		mouse.setpos(x+1, y+1);
		engine.redraw;
	}
	suwakpos(real r)	{	.ansetcpos(xpos + r*(expos-xpos), ypos);	}
	
	mend	{
		ccs.buildfromscreen;
