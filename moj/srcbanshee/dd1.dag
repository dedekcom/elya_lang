}

class SoundSource	{
	init	{	}
	setvol(string ssnd, int srcvol, int outvol, real rad, real length)	{
		if( length.abs>=rad ) <ssnd>.setvol(outvol);
		else {
			srcvol = (1.0-length.abs/rad)*srcvol + outvol;
			if( srcvol > 100 ) srcvol=100;
			<ssnd>.setvol(srcvol);
		}
	}
}

