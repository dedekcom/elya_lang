 <san>.setframe(1,0);
 <san>.ansetbpos(i0, i1);
 string s[2];
 
 real r3 = 0.0;
 speed = speed.abs;
 i0 = 0;
 string sprev = null;
 for( real r2 = rkatr; r2<=rkatl; r2+=r3 ) {
  real r4 = ((r2-rkatr)/(rkatl-rkatr))->abs;
  
  if( r4._rin1(0.1) ) r3 = 0.4*speed;
  else if ( r4._rin1(0.2) ) r3 = 0.8*speed;
