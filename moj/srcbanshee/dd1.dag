	_ifcheck	{
		bool b[2];
		string s[2];
		int i1 = istart;
		ifst = 0;	// initial state
		while(i1<istop)	{
			s0 = <_db>.get(irow, i1);
			s1 = <_db>.dbchecknext(irow, i1);
			match( s1 )	{
				"=", "!=", "<", "<=", ">", ">=" => {
					i1+=2;
					match( s1 )	{
						"="   => b1 =._getifvar(s0) == <_db>.get(irow, i1);
						"!="  => b1 =._getifvar(s0) != <_db>.get(irow, i1);
						"<"   => b1 =._getifvar(s0)->to_r < <_db>.get(irow, i1)->to_r;
						"<="  => b1 =._getifvar(s0)->to_r <= <_db>.get(irow, i1)->to_r;
						">"   => b1 =._getifvar(s0)->to_r > <_db>.get(irow, i1)->to_r;
						">="  => b1 =._getifvar(s0)->to_r >= <_db>.get(irow, i1)->to_r;
						? => ;
					}
					b0 = ._ifst(b1, b0);
					ifst = ._oper_check( <_db>.dbchecknext(irow, i1) );
					if( ifst ) i1++;
				}
				"&" => { i1++;
					b0 = ._ifst(._is_ifvar(s0), b0);
					ifst=1;
				}
				"|" => { i1++;
					b0 = ._ifst(._is_ifvar(s0), b0);
					ifst=2;
				}
				? => {
					b0 = ._ifst(._is_ifvar(s0), b0);
				}
			}
			i1++;
		}
		<_sstarter>.bif=(b0);
		b0;
	}
