					if( idclick==0 )	{
						idclick++;
						.mmask(this);
						fxclickoblok.play;
					} else if (idclick==1)	{
						if( sclicked1!=this )	{
							fxclickoblok.play;
							idclick++;
							.mmask(this);
							sgmstate = "check";
							timcheck.play;
						}
					}
				}
