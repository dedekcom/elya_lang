	public setscreen(int x1, int y1, int x2, int y2)	{
		rleft=x1;	rtop=y1;
		rright=x2;	rbottom = y2;
		rborx = x2;
		rbory = y2;
		_lastx = x1;
		_lasty = y1;
		icamx = (x1+x2)/2;
		icamy = (y1+y2)/2;
		//grbgrs.startpos(x1,y1);
