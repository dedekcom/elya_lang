	itemhide(){
		sFocusItem = null;
		sItem = null;
		imitem.stop(false);
		imitem.hide();
		sItemMover = null;
	}
	printstate	{
		sState.print;
		bskip.print;
	}
	itemclick()	{
		this.skip();
		if ( sState == "closed" ){
			this.pockettoggle();
		} else if ( sState == "opened" ){
			if ( sFocusItem != null ){
				if( sItem!=null )	{
					.pockettoggle;
					_b_rewind = !.playbehif( "beh_"+_sanim+"_USE_"+sItem.strsubbs(_sbase)+"_ON_"+sFocusItem.strsubbs(_sbase) );
					.itemhide;
				} else {
					sItem = sFocusItem;
					imitem.copy(sItem);
					<sFocusItem>.hide();
					//imitem.play("out");
					imitem.play(1);
					imitem.setz( this.getz() + 1 );
					this.setitempos();
					sItemMover = this;
					//this.pockettoggle();
				}
			} else {
				this.pockettoggle();
			}
		} else if ( sState == "open" ){
			this.pockettoggle();
		} else if ( sState == "close" ){
			this.pockettoggle();
		}
	}
	preparetosort(int x, int y)	{
		int id = gmimgvec::isin(x,y,1,1);
