class classadvobject	{
	init()	{
		this.addasadvobject();
	}
	addasadvobject()	{
		if( !AdventureGame ) return;
		string s = this.getname();
		grmsmove.add(s);
		grmslrel.add(s);
		grmslclick.add(s);
		grmsrclick.add(s);
	}
	removefromadvobjs()	{
		if( !AdventureGame ) return;
		string s = this.getname();
		grmsmove.remove(s);
		grmslrel.remove(s);
		grmslclick.remove(s);
		grmsrclick.remove(s);
	}
	virtual onmousemove(int x, int y)	{}
	virtual onmouselclick(int x, int y)		{}
	virtual onmouselrel(int x, int y)		{}
	virtual onmouserclick(int x, int y)		{}
}
