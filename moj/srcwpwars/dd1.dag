	public walkto(string simg, int x, int y)	{
		if( simg!=null )	{
			.playbeh("beh_anhilary_OPERATE_"+<simg>.getbutname);
		}
		return;
		int ile = vecpt.size();
		if( ile<=0 )	{	return;	}
		string sfirst = vecpt.get(0);
		<sfirst>.advwalkto( simg, x, y );
		int i[2], string s;
		for( int i=1; i<ile; i++ )	{
			s = vecpt.get(i);
			<s>.advwalkto( simg, x, y );
			i0 = <s>.getgover(i*iverlen);
			if( i0 >= 0 )	{
				<s>.advwalkto( null, <s>.getxver(i0), <s>.getyver(i0) );
			} else {
				<s>.stop(false);
				<s>.herostand();
			}
