	public move(real x, real y)	{
		rpx+=x;	rpy+=y;
		int ix = rpx, int iy = rpy;
		rpx-=ix;	rpy-=iy;
		if( ix || iy )	gmobjvec::move(ix,iy);
	}
	public movebgr(real x, real y)	{	.move( scalex*x, scaley*y );	}
}

